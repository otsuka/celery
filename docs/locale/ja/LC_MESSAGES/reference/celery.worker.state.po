msgid "celery.worker.state"
msgstr ""

#: ../../../celery/worker/state.pydocstring of celery.worker.state:4
msgid "Internal worker state (global)"
msgstr ""

#: ../../../celery/worker/state.pydocstring of celery.worker.state:6
msgid ""
"This includes the currently active and reserved tasks, statistics, and "
"revoked tasks."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.SOFTWARE_INFO:1
msgid "Worker software/platform information."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.reserved_requests:1
msgid "set of all reserved :class:`~celery.worker.job.Request`'s."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.active_requests:1
msgid "set of currently active :class:`~celery.worker.job.Request`'s."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.total_count:1
msgid "count of tasks accepted by the worker, sorted by type."
msgstr ""

#: ../../../celery/worker/state.pydocstring of celery.worker.state.revoked:1
msgid "the list of currently revoked tasks.  Persistent if statedb set."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.task_reserved:1
#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.task_reserved:1
msgid "Update global state when a task has been reserved."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.task_accepted:1
msgid "Updates global state when a task has been accepted."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.task_ready:1 ../../../celery/worker/state.pydocstring
#: of celery.worker.state.task_ready:1
msgid "Updates global state when a task is ready."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.Persistent:1
msgid ""
"This is the persistent data stored by the worker when :option:`--statedb` is"
" enabled."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.Persistent:4
msgid "It currently only stores revoked task id's."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.Persistent.compress:1
msgid "compress(string[, level]) -- Returned compressed string."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.Persistent.compress:3
msgid "Optional arg level is the compression level, in 0-9."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.Persistent.decompress:1
msgid "decompress(string[, wbits[, bufsize]]) -- Return decompressed string."
msgstr ""

#: ../../../celery/worker/state.pydocstring of
#: celery.worker.state.Persistent.decompress:3
msgid ""
"Optional arg wbits is the window buffer size.  Optional arg bufsize is the "
"initial output buffer size."
msgstr ""
